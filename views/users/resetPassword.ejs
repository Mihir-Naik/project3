<div id="middle" class="col-sm-4 col-sm-offset-4" style="margin-top:30px;">
  
<h1>Reset Password</h1>

<form action="/reset_password?_method=PATCH" method="post">
  <div class="form-group">
    <label for="password">Password</label>
    <input type="password" class="form-control" name="password" id="password" placeholder="xxxxxxxx">
  </div>
  <div class="form-group">
    <label for="confirmPassword">Confirm Password</label>
    <input type="password" class="form-control" name="confirmPassword" placeholder="xxxxxxxx">
  </div>
  <button type="submit" class="btn btn-partmint">Submit</button>
</form>
</div>