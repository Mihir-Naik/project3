<h1>My Properties</h1>

<% if(!_.isEmpty(properties)) { %>
  <ul>
  <% properties.forEach(function(p) { %>
    <li><a href="/properties/<%= p._id %>"><%= p.aptNumber %>, <%= p.street %>, <%= p.city %>, <%= p.zipCode %>, <%= p.state %> <%= p.vacant? '(Vacant)' : '' %></a></li>
  <% }) %>
  </ul>
<% } else { %>
  <h3>You currently have no properties to display</h3>
  <a href="/properties/new">Add new property</a>
<% }%>